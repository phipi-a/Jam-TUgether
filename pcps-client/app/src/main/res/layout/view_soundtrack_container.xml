<?xml version="1.0" encoding="utf-8"?>
<de.pcps.jamtugether.ui.soundtrack.views.SoundtrackContainer xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray">

    <de.pcps.jamtugether.ui.soundtrack.views.SoundtrackView
        android:id="@+id/soundtrack_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <de.pcps.jamtugether.ui.soundtrack.views.SoundtrackNavigationLine
        android:id="@+id/soundtrack_navigation_line"
        android:layout_width="@dimen/soundtrack_navigation_line_width"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</de.pcps.jamtugether.ui.soundtrack.views.SoundtrackContainer>